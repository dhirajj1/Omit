<!DOCTYPE html>
<html>
<head>
  <title>Coming Soon</title>
  <%= stylesheet_link_tag  'application', media: 'all', 'date-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous">
  <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-MrcW6ZMFYlzcLA8Nl+NtUVF0sA7MsXsP1UyJoMp4YLEuNSfAP+JcXn/tWtIaxVXM" crossorigin="anonymous"></script>
</head>
<body id="body">

  <section class="navbar-section" id="home">
      <header class="navbar">
        <div class="container container_navbar">
          <%= image_tag("fulllogo_transparent.png", height: '250px', width: '250px') %>
        </div>
      </header>
  </section>        

  <div class="container">
    <div class="row">
      <div class="col-lg-6 col-md-6">
        <p class="coming">Coming Soon</p>
      </div>
      <div class="col-lg-6 col-md-6">
        <div class="img">
          <%= image_tag("asd.webp", width: '800px') %>
        </div>  
      </div>  
    </div>
  </div>  
     
      
<%= yield %>

</body>
</html>
      
       
        
      


